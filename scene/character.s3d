#ifndef CHARACTER_H
#define CHARACTER_H

class Character
{
	var m_parts;
	var m_idRoot;
	
	get(idPart);
	getParts();
	getRoot();
};

function Character::Character(parts, id) {
	m_parts = parts;
	m_idRoot = id;
}

function Character::get(idPart) {
	return m_parts[idPart];
}

function Character::getParts() {
	return m_parts;
}

function Character::getRoot() {
	return m_parts[m_idRoot];
}

#endif // CHARACTER_H
